@page "/"
@using Orleans.Grpc.Shared.Services.Protos
@using Google.Protobuf
@inject KeyValueStoreService.KeyValueStoreServiceClient client
<h1>Hello, world!</h1>
@code {
    protected override async Task OnInitializedAsync()
    {
        var result =  await this.client.AddColumnAsync(new AddColumnRequest()
        {
            Definition = new ColumnDefinition()
            {
                ColumnName = "Age"
            },
            UUID = ByteString.CopyFrom(Guid.NewGuid().ToByteArray())
        });
    }
   

}